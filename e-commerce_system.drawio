        <mxCell id="3kG8-EqqhI7AJRAp4dw--4" value="&lt;b&gt;Analytics Service&lt;br&gt;(only via Admin)&lt;/b&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
