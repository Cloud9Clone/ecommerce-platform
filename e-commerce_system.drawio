        <mxCell id="tnH4ttXlhKEU7Pji1RbM-17" value="Kafka&lt;br&gt;SASL + SSL" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
